<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

	<bean id="batchExecuterScript" parent="baseJavaScriptExtension"
          class="nl.ciber.alfresco.repo.jscript.batchexecuter.ScriptBatchExecuter">
		<property name="extensionName" value="batchExecuter"/>
		<property name="serviceRegistry" ref="ServiceRegistry"/>
        <property name="actorSystem" ref="batchExecuterActorSystem"/>
	</bean>

    <bean id="batchExecuterActorSystem"
          class="nl.ciber.alfresco.repo.jscript.batchexecuter.ActorSystemWrapper"/>

</beans>
